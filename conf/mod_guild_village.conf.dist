#================================================#
# Guild Village – LOOT ZA AKTIVITY / LOOT SYSTEM #
#================================================#

# CS: Povolit drop měn z custom obsahu?
#     1 = povoleno, 0 = zakázáno
# EN: Enable currency loot from custom sources?
#     1 = enabled, 0 = disabled
GuildVillage.Loot.Enabled = 1

# CS: Hlásit hráči získanou měnu ("+X materiálu") v okamžiku dropu?
# EN: Notify player about gained currency ("+X material") on drop?
GuildVillage.Loot.Notify = 1

# CS: Omezit drop jen na lokaci vesnice
#     1 = padá jen v rámci mapy 37 / zóny 268
#     0 = padá všude (podle tabulek)
# EN: Restrict currency drops to the village zone only
#     1 = drop only on map 37 / zone 268
#     0 = global (subject to DB)
GuildVillage.Loot.OnlyInVillage = 1

# CS: Debug výpisy pro loot systém (spam do logu / chatu).
# EN: Verbose debug output for loot system (log / chat spam).
GuildVillage.Loot.Debug = 0


#=================================================#
# Guild Village – PRODUKCE MATERIÁLŮ / PRODUCTION #
#=================================================#

# CS: Základní produkce bez jakéhokoliv upgradu.
#     Jeden "aktivní slot" generuje materiál po určité době.
# EN: Base production without any upgrades.
#     One "active slot" generates material over time.
GuildVillage.Production.BasePeriodHours = 8
GuildVillage.Production.BaseAmount      = 5

# CS: Bonus k množství za upgrade rank 1 / 2 / 3.
#     (Výsledné množství = BaseAmount + BonusX)
# EN: Extra produced amount for upgrade rank 1 / 2 / 3.
#     (Final amount = BaseAmount + BonusX)
GuildVillage.Production.AmountBonus1    = 3
GuildVillage.Production.AmountBonus2    = 5
GuildVillage.Production.AmountBonus3    = 7

# CS: Zrychlení produkce podle ranku 1 / 2 / 3.
#     Finální perioda = BasePeriodHours * SpeedMultX
#     SpeedMult1 = 6 hodin
#     SpeedMult2 = 4 hodiny
#     SpeedMult3 = 2 hodiny
# EN: Production speed multiplier for upgrade rank 1 / 2 / 3.
#     Final period = BasePeriodHours * SpeedMultX
#     SpeedMult1 = 6 hours
#     SpeedMult2 = 4 hours
#     SpeedMult3 = 2 hours
GuildVillage.Production.SpeedMult1      = 0.75
GuildVillage.Production.SpeedMult2      = 0.50
GuildVillage.Production.SpeedMult3      = 0.25

# CS: Jak často (v minutách) server přepočítá pasivní produkci
#     a uloží ji pro zobrazení (vizuální sync).
#     Spouští se také při otevření gossip menu a při .village info.
# EN: How often (in minutes) the server syncs passive production amounts
#     for display. Also executed on gossip open and .village info.
GuildVillage.Production.WorldTickMinutes = 15


#================================================#
# Guild Village – EXPEDICE / EXPEDITIONS (PVE)   #
#================================================#

# CS: Povolit expedice (nábor hrdinů, posílání na mise)?
# EN: Enable expeditions (hire heroes, send on missions)?
GuildVillage.Expeditions.Enable = 1

# CS: Posílat hráčům hlášky (úspěch, chyba, loot atd.)?
# EN: Send feedback messages to players (success, error, loot etc.)?
GuildVillage.Expeditions.Notify = 1

# CS: Kdo může spravovat expedice (kupovat hrdiny, posílat mise)?
#     1 = GM + Officer
#     0 = pouze Guild Master
# EN: Who can manage expeditions (buy heroes, send missions)?
#     1 = GM + Officer
#     0 = Guild Master only
GuildVillage.Expeditions.OfficersCanManage = 1

# CS: Vyžadovat, aby guildě už patřila vesnice?
#     1 = ano (doporučeno)
#     0 = expedice lze řešit i bez vlastnictví vesnice
# EN: Require that the guild already owns a village?
#     1 = yes (recommended)
#     0 = expeditions work even without a village
GuildVillage.Expeditions.RequireVillage = 1

# CS: Minimální stáří člena v guildě (dny),
#     aby mu šel odeslat loot z guild loot banky.
#     0 = bez omezení.
# EN: Minimum member age in guild (days)
#     required before they can receive loot from the guild loot bank.
#     0 = no restriction.
GuildVillage.Expeditions.LootMinMemberAgeDays = 28


